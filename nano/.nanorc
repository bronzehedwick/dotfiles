set tabsize 4
set tabstospaces
set speller "/usr/local/bin/aspell -x -c"
set softwrap

bind M-F nextword main
bind M-B prevword main

syntax "Email" ".*.mail"
color magenta "^>([^>].*|$)"
color blue "^> ?>([^>].*|$)"
color green "^> ?> ?>([^>].*|$)"
color cyan "^> ?> ?> ?>.*"
color yellow "^> ?> ?> ?> ?>.*"
